    {
      "Text": {
        "Text": "Very well.  We'll tell him to expect you, then.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 1546
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": 10,

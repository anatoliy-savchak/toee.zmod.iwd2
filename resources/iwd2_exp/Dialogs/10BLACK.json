    {
      "Text": {
        "Text": "I believe I heard your mother say much the same thing.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 1540
      },
      "JournalIndex": 0,

    {
      "Text": {
        "Text": "You as well.  Pleasant sailing.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 1387
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

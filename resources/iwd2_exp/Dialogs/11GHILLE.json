    {
      "Text": {
        "Text": "I will.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28440
      },
      "JournalIndex": 0,

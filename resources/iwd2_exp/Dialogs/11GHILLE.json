      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 22,
      "Flags": "HasText, HasNextDialog"
    },
    {
      "Text": {
        "Text": "We're still breathing... for now.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28436
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

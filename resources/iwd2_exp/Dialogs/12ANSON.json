      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 38,
      "Flags": "HasText, HasNextDialog"
    },
    {
      "Text": {
        "Text": "Because I told her.  She deserved to know.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28141
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "12Anson",
      "NextPhraseIndex": 20,
      "Index": 39,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "I don't know.  She's an old woman - she hears a lot of things.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28142
      },
      "JournalIndex": 0,
      "TriggerIndex": 17,

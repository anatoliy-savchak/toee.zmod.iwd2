      "NextScriptDialog": "12Black",
      "NextPhraseIndex": 4,
      "Index": 8,
      "Flags": "HasText, HasAction, HasJournal"
    },
    {
      "Text": {
        "Text": "Perhaps so.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 31633
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 9,
      "Flags": "HasText, HasNextDialog"
    },
    {
      "Text": {
        "Text": "Just get out of my sight before I regret not striking you down in the Salty Dog. ",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 31634

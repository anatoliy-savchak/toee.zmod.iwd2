    },
    {
      "Text": {
        "Text": "Thanks, Kadence - and next time, don't take bets you can't win.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 15315
      },
      "JournalIndex": 0,

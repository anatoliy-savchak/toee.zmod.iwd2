      "Flags": "HasText, HasTrigger, HasAction, HasNextDialog"
    },
    {
      "Text": {
        "Text": "I can't accept this, Kadence; I did this not for the sake of profit.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 36150

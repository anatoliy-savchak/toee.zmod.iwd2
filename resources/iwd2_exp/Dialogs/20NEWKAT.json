      "NextScriptDialog": "20NewKat",
      "NextPhraseIndex": 19,
      "Index": 52,
      "Flags": "HasText, HasAction, HasJournal"
    },
    {
      "Text": {
        "Text": "I must take my leave of you.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 36004
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 53,
      "Flags": "HasText, HasNextDialog"

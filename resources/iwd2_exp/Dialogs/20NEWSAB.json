      "NextScriptDialog": "20NewSab",
      "NextPhraseIndex": 8,
      "Index": 18,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "What matters?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35916
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "20NewSab",
      "NextPhraseIndex": 8,
      "Index": 19,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Yes.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35918
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "20NewSab",
      "NextPhraseIndex": 9,
      "Index": 20,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "That is my concern, not yours.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35919
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "20NewSab",
      "NextPhraseIndex": 9,
      "Index": 21,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "What kind of trap?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35921
      },
      "JournalIndex": 35892,
      "TriggerIndex": -1,
      "ActionIndex": 5,
      "NextScriptDialog": "20NewSab",
      "NextPhraseIndex": 10,
      "Index": 22,
      "Flags": "HasText, HasAction, HasJournal"
    },
    {
      "Text": {
        "Text": "Do you know where this lookout can be found?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,

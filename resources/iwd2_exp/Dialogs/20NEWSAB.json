      "NextPhraseIndex": 18,
      "Index": 17,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "My time is valuable, lady.  Make haste with what you have to say.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35914
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

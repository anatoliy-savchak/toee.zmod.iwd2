        "PitchVariance": 0,
        "Strref": 36502
      },
      "JournalIndex": 36490,
      "TriggerIndex": 1,
      "ActionIndex": 1,
      "NextScriptDialog": "21Xuki",
      "NextPhraseIndex": 16,
      "Index": 3,
      "Flags": "HasText, HasTrigger, HasAction, HasJournal"

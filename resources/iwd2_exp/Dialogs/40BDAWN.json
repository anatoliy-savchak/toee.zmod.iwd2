    {
      "Text": {
        "Text": "It doesn't look vacant to me; it looks like it's been used recently.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 17963
      },
      "JournalIndex": 0,
      "TriggerIndex": 13,

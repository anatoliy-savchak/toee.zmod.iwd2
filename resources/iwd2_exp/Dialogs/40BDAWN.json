      "NextScriptDialog": "40Bdawn",
      "NextPhraseIndex": 3,
      "Index": 5,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "Have I told you you've set your camp around a wolves' den?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 17949
      },
      "JournalIndex": 0,
      "TriggerIndex": 5,

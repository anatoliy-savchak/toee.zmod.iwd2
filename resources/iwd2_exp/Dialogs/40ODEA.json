      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "I've nothing more to say to you.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18611
      },

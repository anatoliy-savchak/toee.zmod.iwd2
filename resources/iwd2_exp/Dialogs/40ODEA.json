      "Index": 9,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "May we speak to Illium?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18616
      },
      "JournalIndex": 0,
      "TriggerIndex": 5,
      "ActionIndex": 3,
      "NextScriptDialog": "40Odea",
      "NextPhraseIndex": 3,
      "Index": 10,
      "Flags": "HasText, HasTrigger, HasAction"
    },
    {
      "Text": {
        "Text": "Take us to Illium immediately, or I swear we will level your *precious* Andora.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18624
      },
      "JournalIndex": 0,
      "TriggerIndex": 6,
      "ActionIndex": -1,
      "NextScriptDialog": "40Odea",
      "NextPhraseIndex": 13,
      "Index": 11,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "What's Andora?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18817
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": 4,
      "NextScriptDialog": "40Odea",
      "NextPhraseIndex": 5,
      "Index": 12,
      "Flags": "HasText, HasAction"
    },
    {
      "Text": {
        "Text": "We only wish to pass through peacefully.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18824
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "40Odea",
      "NextPhraseIndex": 4,
      "Index": 13,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "I've nothing more to say to you.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 18611
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "40Odea",
      "NextPhraseIndex": 6,
      "Index": 14,
      "Flags": "HasText"
    },
    {

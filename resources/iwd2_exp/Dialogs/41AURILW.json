    },
    {
      "Text": {
        "Text": "Well met again, my brother!  Go with Auril.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 21875
      },
      "ResponsesIndexFirst": 5,

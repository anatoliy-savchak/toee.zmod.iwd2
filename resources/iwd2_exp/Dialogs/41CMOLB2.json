{
  "Phrases": [
    {
      "Text": {
        "Text": "G'day.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 40993
      },
      "ResponsesIndexFirst": 0,

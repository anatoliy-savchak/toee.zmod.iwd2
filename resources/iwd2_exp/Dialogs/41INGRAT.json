    },
    {
      "Text": {
        "Text": "Ingrath - I brought this letter from Garradun, for his fiance.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 975
      },
      "JournalIndex": 961,
      "TriggerIndex": 1,

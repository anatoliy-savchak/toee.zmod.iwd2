      "NextScriptDialog": "41Oria",
      "NextPhraseIndex": 6,
      "Index": 8,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Break it.  What do we care?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 31910
      },
      "JournalIndex": 31895,

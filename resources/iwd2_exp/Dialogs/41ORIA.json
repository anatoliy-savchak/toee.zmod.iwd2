    {
      "Text": {
        "Text": "Our apologies for to disturbing you.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 31902
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "41Oria",
      "NextPhraseIndex": 9,
      "Index": 3,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "We agree, but we'll be the teacher!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 31903
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "41Oria",
      "NextPhraseIndex": 9,
      "Index": 4,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": null,
        "Flags": "NoMessageData",

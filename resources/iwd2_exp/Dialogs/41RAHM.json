      "Flags": "None"
    },
    {
      "Text": {
        "Text": "We're heading to Kuldahar via the Eastern Pass - what will you two do now?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 7492
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

      "NextScriptDialog": "50Hadbru",
      "NextPhraseIndex": 2,
      "Index": 0,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "We *are* the army.  Out of our way, barbarian.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 7751
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "50Hadbru",
      "NextPhraseIndex": 1,
      "Index": 1,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "We don't have time for this.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 7752
      },

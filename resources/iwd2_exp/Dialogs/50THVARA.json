    {
      "Text": {
        "Text": null,
        "Flags": "NoMessageData",
        "Sound": null,
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 0
      },
      "JournalIndex": 0,
      "TriggerIndex": 7,
      "ActionIndex": 0,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 17,
      "Flags": "HasTrigger, HasAction, HasNextDialog"
    },
    {
      "Text": {
        "Text": "You'll run off if you know what's good for you, giant... we're not leaving anyone here alive.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 37851
      },
      "JournalIndex": 0,
      "TriggerIndex": 8,
      "ActionIndex": 1,
      "NextScriptDialog": "50GNTFO",
      "NextPhraseIndex": 5,
      "Index": 18,
      "Flags": "HasText, HasTrigger, HasAction"
    },
    {
      "Text": {
        "Text": "We'll see who the idiots are when the battle is over, Thvara.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 37852

    {
      "Text": {
        "Text": "No, I only wish to speak to you.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 5725
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

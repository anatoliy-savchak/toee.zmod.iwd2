      "Index": 20
    },
    {
      "Text": {
        "Text": "What are you doing, outsider?  Get rid of the monsters you've tracked in here!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 36481
      },
      "ResponsesIndexFirst": 55,

    {
      "Text": {
        "Text": "Wait - who are you?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35624
      },
      "JournalIndex": 35616,
      "TriggerIndex": 0,
      "ActionIndex": -1,
      "NextScriptDialog": "53DroPr",
      "NextPhraseIndex": 5,
      "Index": 4,
      "Flags": "HasText, HasTrigger, HasJournal"
    },
    {
      "Text": {
        "Text": "You must be the Lolthian assassins Viizra told us about...",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35625
      },
      "JournalIndex": 35617,
      "TriggerIndex": 1,
      "ActionIndex": -1,
      "NextScriptDialog": "53DroPr",
      "NextPhraseIndex": 7,
      "Index": 5,
      "Flags": "HasText, HasTrigger, HasJournal"
    },
    {
      "Text": {
        "Text": "There's no cause for battle, sister.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35626
      },
      "JournalIndex": 0,
      "TriggerIndex": 2,
      "ActionIndex": -1,
      "NextScriptDialog": "53DroPr",
      "NextPhraseIndex": 6,
      "Index": 6,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "Let's go our separate ways without a fight.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 35627
      },
      "JournalIndex": 0,
      "TriggerIndex": 3,

      "NextPhraseIndex": 2,
      "Index": 2,
      "Flags": "HasText, HasTrigger, HasAction, HasJournal"
    },
    {
      "Text": {
        "Text": "You are an abomination of nature.  It is my duty to see that your existence does not continue.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22282
      },
      "JournalIndex": 22278,

      "NextPhraseIndex": 3,
      "Index": 2,
      "Flags": "HasText, HasTrigger, HasAction, HasJournal"
    },
    {
      "Text": {
        "Text": "Adventurers?  No, we're... explorers.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22321
      },
      "JournalIndex": 22313,
      "TriggerIndex": 3,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 4,
      "Index": 3,
      "Flags": "HasText, HasTrigger, HasJournal"
    },
    {
      "Text": {
        "Text": "We are from the Ten-Towns.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22327
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 8,
      "Index": 4,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "How did you know this was going to happen?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22324
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 5,
      "Index": 5,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "We have spoken with your brother, Malavon.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22325
      },
      "JournalIndex": 0,
      "TriggerIndex": 4,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 7,
      "Index": 6,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "She is dead.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22331
      },
      "JournalIndex": 0,
      "TriggerIndex": 5,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 10,
      "Index": 7,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "I do not know her fate.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22332
      },
      "JournalIndex": 0,
      "TriggerIndex": 6,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 10,
      "Index": 8,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "Her fate is unimportant.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22334
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 10,
      "Index": 9,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "You don't seem pleased about what's happening.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22336
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "53Ginafa",
      "NextPhraseIndex": 5,
      "Index": 10,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Yes, we are adventurers, and we have come to destroy the Viciscamera.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 22342
      },

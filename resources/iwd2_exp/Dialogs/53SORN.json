      "ActionIndex": 4,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 16,
      "Flags": "HasAction, HasNextDialog"
    },
    {
      "Text": {
        "Text": null,
        "Flags": "NoMessageData",
        "Sound": null,
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 0
      },

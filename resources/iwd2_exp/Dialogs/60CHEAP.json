      "NextPhraseIndex": 9,
      "Index": 22,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "Why are you here?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,

      "Index": 0,
      "Flags": "HasText, HasTrigger, HasAction"
    },
    {
      "Text": {
        "Text": "You are some sort of air elemental... bound to this place.  Why?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 38577
      },
      "JournalIndex": 0,
      "TriggerIndex": 1,

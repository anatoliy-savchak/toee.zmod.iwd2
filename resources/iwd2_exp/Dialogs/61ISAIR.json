      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Not really.  The place is still standing, after all.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 29471
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "61Isair",
      "NextPhraseIndex": 11,
      "Index": 41,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Isair, let us cut the idle conversation and simply end this.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,

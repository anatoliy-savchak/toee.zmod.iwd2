    {
      "Text": {
        "Text": "May I ask a question?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 36089
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

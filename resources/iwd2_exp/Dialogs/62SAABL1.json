      "NextScriptDialog": "62Saabl1",
      "NextPhraseIndex": 13,
      "Index": 24,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Enough of this!  Prepare to die!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 9150
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": 7,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 25,
      "Flags": "HasText, HasAction, HasNextDialog"
    },
    {
      "Text": {
        "Text": "How was Malavon supposed to do that?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,

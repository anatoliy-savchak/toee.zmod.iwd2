      "NextPhraseIndex": 3,
      "Index": 4,
      "Flags": "HasText, HasTrigger, HasAction"
    },
    {
      "Text": {
        "Text": "Do you prepare the meals for the guard barracks as well?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 29537
      },
      "JournalIndex": 0,
      "TriggerIndex": 1,

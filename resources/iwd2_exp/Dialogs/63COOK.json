      "NextPhraseIndex": 1,
      "Index": 0,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "May I ask what you are doing here?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 29532
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

    },
    {
      "Text": {
        "Text": "What are you doing here?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26009
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 1,
      "Index": 20,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Can you tell me about this place?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26010
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 8,
      "Index": 21,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "I would like to speak with the foreman.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26011
      },
      "JournalIndex": 0,
      "TriggerIndex": 1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 10,
      "Index": 22,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "I see no guards here.  Why do you not escape?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26035
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 12,
      "Index": 23,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "I must take my leave of you.  Farewell.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26013
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "",
      "NextPhraseIndex": 0,
      "Index": 24,
      "Flags": "HasText, HasNextDialog"
    },
    {
      "Text": {
        "Text": "It should be a simple matter for me to deal with the demon.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26037
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 13,
      "Index": 25,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Where can I find him?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 26039
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "63Vashti",
      "NextPhraseIndex": 10,
      "Index": 26,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "What are you doing here?",

    },
    {
      "Text": {
        "Text": "Foul beast!  Back to the burning fires of the Abyss with you!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 36761
      },

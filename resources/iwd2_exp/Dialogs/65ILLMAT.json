{
  "Phrases": [
    {
      "Text": {
        "Text": "Standing before you is a twisted, terrified statue of Ilmater.  There looks to be enough space on the pedestal to put the items for the Ilmater ritual.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 24397
      },
      "ResponsesIndexFirst": 0,
      "ResponsesCount": 2,
      "TriggerIndex": 7,
      "Index": 0
    },
    {
      "Text": {
        "Text": "The items that you placed on the pedestal begin to glow a soft blue.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 24401
      },
      "ResponsesIndexFirst": 2,
      "ResponsesCount": 1,
      "TriggerIndex": -1,
      "Index": 1
    },
    {
      "Text": {
        "Text": "\"I suffer unto you, Ilmater, for the talons of the impure and unjust have captured the souls of the weak and hurt...\"",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 24404
      },
      "ResponsesIndexFirst": 3,
      "ResponsesCount": 1,
      "TriggerIndex": -1,
      "Index": 2
    },
    {
      "Text": {
        "Text": "You do not have all the components to perform the Ilmater Sanctification Ritual.",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 24406
      },
      "ResponsesIndexFirst": 4,
      "ResponsesCount": 1,

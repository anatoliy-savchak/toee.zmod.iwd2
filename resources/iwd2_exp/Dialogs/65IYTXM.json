      "NextScriptDialog": "65IytXm",
      "NextPhraseIndex": 1,
      "Index": 0,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "Your threats are meaningless to me; your time here is at an end!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 27260
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

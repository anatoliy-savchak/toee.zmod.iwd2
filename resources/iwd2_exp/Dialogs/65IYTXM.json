      "NextScriptDialog": "65IytXm",
      "NextPhraseIndex": 2,
      "Index": 1,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "How do you know my name?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28534
      },
      "JournalIndex": 0,
      "TriggerIndex": 0,
      "ActionIndex": -1,
      "NextScriptDialog": "65IytXm",
      "NextPhraseIndex": 3,
      "Index": 2,
      "Flags": "HasText, HasTrigger"
    },
    {
      "Text": {
        "Text": "I will not make a deal with the likes of you.  Be gone!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28536
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

    {
      "Text": {
        "Text": "Who speaks?!",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 27259
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,

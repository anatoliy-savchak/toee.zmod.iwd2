      "NextScriptDialog": "65IytXm",
      "NextPhraseIndex": 2,
      "Index": 6,
      "Flags": "HasText"
    },
    {
      "Text": {
        "Text": "That could be problematic.  What do you propose?",
        "Flags": "Standard",
        "Sound": "",
        "VolumeVariance": 0,
        "PitchVariance": 0,
        "Strref": 28763
      },
      "JournalIndex": 0,
      "TriggerIndex": -1,
      "ActionIndex": -1,
      "NextScriptDialog": "65IytXm",
      "NextPhraseIndex": 5,
      "Index": 7,
      "Flags": "HasText"
    },
    {

		MarkSpellAndObject("13121216120323061101122111021111",LastMarkedObject,SPELLCAST_IGNORE_SEE | SPELLCAST_IGNORE_VALID_SPELL_TARGET | SPELLCAST_RANDOM)  // [CLERIC_STRENGTH_OF_ONE, CLERIC_ALICORN_LANCE, CLERIC_CHANT, WIZARD_HOLD_PERSON, CLERIC_BLESS, CLERIC_RAINSTORM, CLERIC_COMMAND, CLERIC_BANE]
		MarkSpellAndObject("11141117135012501150120411161113",LastMarkedObject,SPELLCAST_IGNORE_SEE | SPELLCAST_IGNORE_VALID_SPELL_TARGET | SPELLCAST_RANDOM)  // [CLERIC_DOOM, CLERIC_FROST_FINGERS, CLERIC_SUMMON_NATURES_ALLY_3, CLERIC_SUMMON_NATURES_ALLY_2, CLERIC_SUMMON_NATURES_ALLY_1, CLERIC_CHARM_PERSON_OR_MAMMAL, CLERIC_FAERIE_FIRE, CLERIC_SUNSCORCH]
		Continue()
END

IF
	!IsMarkedSpell(MARKED_SPELL)
THEN
	RESPONSE #100
		Spell(SpellTarget,MARKED_SPELL)
		WaitAnimation(Myself,WALK)
		WaitAnimation(Myself,CONJURE)
		WaitAnimation(Myself,CAST)
		ForceMarkedSpell(MARKED_SPELL)
END

IF
	See(NearestEnemyOf(Myself),0)
THEN
	RESPONSE #100
		SetBestWeapon(LastMarkedObject,3)
		AttackReevaluate(LastMarkedObject,45)
END

IF
	!See(NearestEnemyOf(Myself),0)
	Global("Player","LOCALS",-1)
THEN
	RESPONSE #100
		MoveToObjectUntilSee(NearestPC)
END

IF
	True()
THEN
	RESPONSE #100
		NoAction()
END


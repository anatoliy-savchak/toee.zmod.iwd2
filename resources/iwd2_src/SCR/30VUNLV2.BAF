IF
	Global("LEAVE_START","LOCALS",0)
	!Global("VUNARG_LEAVING","GLOBAL",0)
THEN
	RESPONSE #100
		SetStartPos([750.1140])
		SetGlobal("LEAVE_START","LOCALS",1)
END

IF
	!Global("VUNARG_LEAVING","GLOBAL",0)
	NearLocation(Myself,3750,1140,4)
THEN
	RESPONSE #100
		DestroySelf()
END

IF
	!Global("TEAM_10","MYAREA",0)
THEN
	RESPONSE #100
		ChangeCurrentScript("")
END

IF
	Global("ML_0","LOCALS",0)
	ActionListEmpty()
	NearLocation(Myself,3750,1140,4)
THEN
	RESPONSE #100
		SetStartPos([3750.1140])
		SetGlobal("ML_0","LOCALS",1)
		Continue()
END

IF
	Global("ML_0","LOCALS",0)
	!Global("VUNARG_LEAVING","GLOBAL",0)
THEN
	RESPONSE #100
		MoveToPoint([3750.1140])
END

IF
	Global("ML_0","LOCALS",1)
	!Global("VUNARG_LEAVING","GLOBAL",0)
THEN
	RESPONSE #100
		DestroySelf()
END


IF
	Global("C_8_MASTERED","MYAREA",0)
	Global("WATCHING_PLAYER","MYAREA",1)
	Dead("06_52MNKBR_1")
	Dead("06_52MNKBR_2")
THEN
	RESPONSE #100
		StartCutSceneMode()
		SetGlobal("C_8_MASTERED","MYAREA",1)
		ClearAllActions()
		MultiPlayerSync()
		Wait(1)
		FadeToColor([0.0],0)
		Wait(1)
		DropInventoryEX(Player1)
		MultiPlayerSync()
		SetGlobal("WATCHING_PLAYER","MYAREA",0)
		ActionOverride(Player1,JumpToSavedLocation())
		MultiPlayerSync()
		SetGlobal("52Morohem_Master","GLOBAL",9)
		Unlock("AR5202_Door10")
		Unlock("AR5202_Door1")
		OpenDoor("AR5202_Door10")
		OpenDoor("AR5202_Door1")
		ActionOverride("10_52Proxi_00",DestroySelf())
		ActionOverride("10_52Proxi_01",DestroySelf())
		ActionOverride("10_52Proxi_02",DestroySelf())
		ActionOverride("10_52Proxi_03",DestroySelf())
		ActionOverride("10_52Proxi_04",DestroySelf())
		ActionOverride("10_52Proxi_05",DestroySelf())
		ActionOverride("10_52Proxi_06",DestroySelf())
		ActionOverride("10_52Proxi_07",DestroySelf())
		ActionOverride("10_52Proxi_08",DestroySelf())
		ActionOverride("10_52Proxi_09",DestroySelf())
		AddXPVar("Level_10_Average",34477)  // You have mastered the Chamber of Immolation, last of the Eight Chambers.
		Wait(1)
		ChangeEnemyAlly(Player2,PC)
		ChangeEnemyAlly(Player3,PC)
		ChangeEnemyAlly(Player4,PC)
		ChangeEnemyAlly(Player5,PC)
		ChangeEnemyAlly(Player6,PC)
		MultiPlayerSync()
		ResetJoinRequests()
		MultiPlayerSync()
		MoveViewObject(Player1,INSTANT)
		MultiPlayerSync()
		MultiPlayerSync()
		FadeFromColor([0.0],0)
		Wait(1)
		MultiPlayerSync()
		EndCutSceneMode()
		ChangeCurrentScript("")
END


IF
	True()
THEN
	RESPONSE #100
		CutSceneId("Malavon_Despana")
		MultiPlayerSync()
		OpenDoor("AR5300_Door1_NorthGate")
		Wait(2)
		MultiPlayerSync()
		MoveViewPoint([610.730],VERY_FAST)
		MoveToPoint([610.730])
		SetSavedLocationPoint(610,730,SE)
		SetStartPos([610.730])
		Wait(4)
		MultiPlayerSync()
		CloseDoor("AR5300_Door1_NorthGate")
		MoveViewObject(NearestPC,15)
		EndCutSceneMode()
		MultiPlayerSync()
END


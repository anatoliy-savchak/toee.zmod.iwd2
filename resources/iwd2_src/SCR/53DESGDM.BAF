IF
	!NearSavedLocation(1)
	!Allegiance(Myself,ENEMY)
	!See(NearestEnemyOf(Myself),0)
	!See([0.0.DRIDER],0)
THEN
	RESPONSE #100
		ReturnToSavedLocation(0)
END

IF
	NearSavedLocation(1)
	!Allegiance(Myself,ENEMY)
	!See(NearestEnemyOf(Myself),0)
	!See([0.0.DRIDER],0)
THEN
	RESPONSE #100
		FaceSavedLocation(Myself)
END


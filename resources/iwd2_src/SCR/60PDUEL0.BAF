IF
	Global("60Duel_VFA","GLOBAL",1)
	Global("60Duel_VFA_State","MYAREA",1)
	Entered([PC])
THEN
	RESPONSE #100
		SetGlobal("60Duel_VFA_State","MYAREA",2)
		ClearAllActions()
END


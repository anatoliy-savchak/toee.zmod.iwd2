IF
	True()
THEN
	RESPONSE #100
		CutSceneId("Vese_Nejj")
		MultiPlayerSync()
		Wait(1)
		SpellHitEffectSprite(Myself,Myself,DIMENSION_DOOR_ORIGIN,0)
		HideCreature(Myself,TRUE)
		JumpToPoint([546.1861])
		Wait(1)
		MoveViewPoint([546.1861],25)
		FaceObject("Cedrin_Zil")
		Wait(5)
		SpellHitEffectSprite(Myself,Myself,DIMENSION_DOOR_DESTINATION,0)
		HideCreature(Myself,FALSE)
		Wait(1)
		FloatMessage(Myself,39364)  // "I have found you guilty of the murder of an Enclave member, Cedrin.  For that, you shall pay with your life!"
		Wait(11)
		ForceSpell("Cedrin_Zil",WIZARD_MORDENKAINENS_FORCE_MISSILES)  // SPWI422.SPL (Mordenkainen's Force Missiles)
		PlaySequence("Cedrin_Zil",DAMAGE)
		Wait(1)
		Kill("Cedrin_Zil")
		Wait(5)
		SpellHitEffectSprite(Myself,Myself,DIMENSION_DOOR_ORIGIN,0)
		HideCreature(Myself,TRUE)
		Wait(3)
		MoveViewPoint([2684.932],25)
		JumpToSavedLocation()
		FaceSavedLocation(Myself)
		SpellHitEffectSprite(Myself,Myself,DIMENSION_DOOR_DESTINATION,0)
		HideCreature(Myself,FALSE)
		Wait(4)
		MultiPlayerSync()
		EndCutSceneMode()
END


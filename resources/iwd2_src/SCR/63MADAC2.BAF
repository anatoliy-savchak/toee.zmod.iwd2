		MarkSpellAndObject("2810",[PC.0.0.0.0.0.0.0.0.CLASSMASK_BARD],SPELLCAST_RANDOM)  // [WIZARD_FLAYING]
		MarkSpellAndObject("2510",[PC.0.0.0.0.0.0.0.0.CLASSMASK_BARD],SPELLCAST_RANDOM)  // [WIZARD_FEEBLEMIND]
		Continue()

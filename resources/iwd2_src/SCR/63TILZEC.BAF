IF
	GlobalTimerExpired("Hide_Ability","LOCALS")
THEN
	RESPONSE #100
		SetGlobalTimer("Hide_Ability","LOCALS",7)
		Hide()
END

IF
	HasItem("00potn05",Myself)  // Potion of Extra Healing
	HPPercentLT(Myself,25)
THEN
	RESPONSE #100
		UseItem("00potn05",Myself)  // Potion of Extra Healing
END

IF
	!StateCheck(Myself,STATE_INVISIBLE)
	IsCreatureHiddenInShadows(Myself)
	See(LastMarkedObject,0)
THEN
	RESPONSE #100
		EquipMostDamagingMelee()
		BackStab(LastMarkedObject)
END

IF
	See(LastMarkedObject,0)
THEN
	RESPONSE #100
		EquipWeapon()
		AttackOneRound(LastMarkedObject)
END

IF
	!StateCheck(Myself,STATE_INVISIBLE)
	IsCreatureHiddenInShadows(Myself)
	See(NearestEnemyOf(Myself),0)
THEN
	RESPONSE #100
		EquipMostDamagingMelee()
		BackStab(LastMarkedObject)
END

IF
	See(NearestEnemyOf(Myself),0)
THEN
	RESPONSE #100
		EquipWeapon()
		AttackOneRound(LastMarkedObject)
END


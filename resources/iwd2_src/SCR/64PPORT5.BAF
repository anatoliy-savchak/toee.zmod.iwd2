	GlobalGT("MT_PortalSequence","GLOBAL",1)
THEN
	RESPONSE #100
		ForceSpellPoint([470.625],WIZARD_AGANNAZAR_SCORCHER)  // SPWI217.SPL (Aganazzar's Scorcher)
		StartTimer(1,4)
END


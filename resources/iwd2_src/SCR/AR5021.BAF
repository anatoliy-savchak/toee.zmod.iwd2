		Continue()
END

IF
	Global("AR5021_STATE","GLOBAL",2)
	Global("AR5003_DARK_TREE_CLEAR","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",0)
		SetGlobal("AR5021_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([400.550]))
		Continue()
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",1)
		SetGlobal("AR5021_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([350.350]))
		Continue()
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",2)
		SetGlobal("AR5021_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([750.400]))
		Continue()
END


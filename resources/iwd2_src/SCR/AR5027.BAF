IF
	!Global("FW_WILL_O_WISPS_DEAD","GLOBAL",0)
	!Dead("TRAP_WISP")
THEN
	RESPONSE #100
		ActionOverride("TRAP_WISP",DestroySelf())
		Continue()
END

IF
	Global("AR5027_STATE","GLOBAL",2)
	Global("AR5003_DARK_TREE_CLEAR","GLOBAL",0)
THEN
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",0)
		SetGlobal("AR5027_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([500.450]))
		Continue()
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",1)
		SetGlobal("AR5027_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([600.550]))
		Continue()
	RESPONSE #100
		SetGlobal("ACTIVE_DARK_TREE","MYAREA",2)
		SetGlobal("AR5027_STATE","GLOBAL",3)
		HideCreature("TRAP_WISP",FALSE)
		ActionOverride("TRAP_WISP",JumpToPoint([650.700]))
		Continue()
END


	Global("ADP_","MYAREA",0)
	HPLT("Undead_Gaernat_Sharptooth",1)
THEN
	RESPONSE #100
		SetGlobal("ADP_","MYAREA",1)
		SpellHitEffectSprite("Undead_Gaernat_Sharptooth","Undead_Gaernat_Sharptooth",MONSTER_SUMMONING_I,0)
		Continue()
END

